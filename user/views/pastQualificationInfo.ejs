<%- include('partials/header2'); -%>

    <div class="progress">
        <div class="progress-bar" role="progressbar" style="width: 67%" aria-valuenow="25" aria-valuemin="0"
            aria-valuemax="100">
            67% Completed
        </div>
    </div>

    <div class="card-body p-4 p-sm-5">
        <form class="row g-3" action="/user/pqi/add" method="post" enctype="multipart/form-data">
            <legend>
                <h2><b>Past Qualifications Information</b></h2>
            </legend>
            <hr />
            <legend>Past Qualifications Details</legend>

            <div class="col-md-4">
                <label for="inputState" class="form-label">Qualification level</label>
                <select id="inputState" name="pqLevel" class="form-select">
                    <option value="School">School</option>
                    <option value="High School">High School</option>
                    <option value="Undergraduate Course">Undergraduate Course</option>
                    <option value="Post Graduate Course">Post Graduate Course</option>
                    <option value="Phd Course">Phd Course</option>
                </select>
            </div>

            <div class="col-md-4">
                <label for="inputZip" class="form-label">Stream</label>
                <select id="inputState" name="pqStream" class="form-select">
                    <option value="Engineering">Engineering</option>
                    <option value="Medical">Medical</option>
                    <option value="Pharmacy">Pharmacy</option>
                    <option value="Sciences">Sciences</option>
                    <option value="Arts">Arts</option>
                    <option value="Commerce">Commerce</option>
                </select>
            </div>

            <div class="col-md-4">
                <label for="inputZip" class="form-label">Completed</label>
                <select id="inputState" name="pqCompleted" class="form-select">
                    <option value="Completed">Completed</option>
                    <option value="Pursuing">Pursuing</option>
                </select>
            </div>

            <div class="col-md-4">
                <label for="inputState" class="form-label">Institute State</label>
                <select id="inputState" name="pqInstituteState" class="form-select">
                    <option value="Maharshtra">Maharshtra</option>
                    <option value="Tamil Nadu">Tamil Nadu</option>
                </select>
            </div>

            <div class="col-md-4">
                <label for="inputZip" class="form-label">Institute District</label>
                <select id="inputState" name="pqInstituteDistrict" class="form-select">
                    <option value="Kolhapur">Kolhapur</option>
                    <option value="Amhednagar">Amhednagar</option>
                    <option value="Pune">Pune</option>
                </select>
            </div>

            <div class="col-md-4">
                <label for="inputZip" class="form-label">Institute Taluka</label>
                <select id="inputState" name="pqInstituteTaluka" class="form-select">
                    <option value="Karveer">Karveer</option>
                    <option value="Ichalkaranji">Ichalkaranji</option>
                    <option value="Hatkalangle">Hatkalangle</option>
                </select>
            </div>

            <div class="col-md-8">
                <label for="inputZip" class="form-label">College Name</label>
                <select id="inputState" name="pqCollegeName" class="form-select">
                    <option value="VIT Pune">VIT Pune</option>
                    <option value="PICT">PICT</option>
                    <option value="VIIT">VIIT</option>
                </select>
            </div>

            <div class="col-md-4">
                <label for="inputZip" class="form-label">Course Name</label>
                <select id="inputState" name="pqCourseName" class="form-select">
                    <option value="Computer Engineering">Computer Engineering</option>
                    <option value="Information Technology">Information Technology</option>
                    <option value="Artificial Intelligence and Data Science">Artificial Intelligence and Data Science</option>
                </select>
            </div>

            <div class="col-md-4">
                <label for="inputZip" class="form-label">Board/University Name</label>
                <select id="inputState" name="pqBoardUniversityName" class="form-select">
                    <option value="Savitribai Phule Pune University">Savitribai Phule Pune University</option>
                    <option value="Shivaji University">Shivaji University</option>
                    <option value="Maharashtra Board">Maharashtra Board</option>
                    <option value="CBSE">CBSE</option>
                </select>
            </div>

            <div class="col-md-4">
                <label for="inputZip" class="form-label">Mode</label>
                <select id="inputState" name="pqMode" class="form-select">
                    <option value="Regular">Regular</option>
                    <option value="Distance">Distance</option>
                </select>
            </div>

            <div class="col-md-4">
                <label for="inputState" class="form-label">Admission Year</label>
                <select id="inputState" name="pqAddmissionYear" class="form-select">
                    <option value="2022">2022</option>
                    <option value="2021">2021</option>
                    <option value="2020">2020</option>
                    <option value="2019">2019</option>
                    <option value="2018">2018</option>
                </select>
            </div>

            <div class="col-md-4">
                <label for="inputState" class="form-label">Passing Year</label>
                <select id="inputState" name="pqPassingYear" class="form-select">
                    <option value="2022">2022</option>
                    <option value="2021">2021</option>
                    <option value="2020">2020</option>
                    <option value="2019">2019</option>
                    <option value="2018">2018</option>
                </select>
            </div>

            <div class="col-md-4">
                <label for="inputState" class="form-label">Result</label>
                <select id="inputState" name="pqResult" class="form-select">
                    <option value="Pass">Pass</option>
                    <option value="Fail">Fail</option>
                </select>
            </div>

            <div class="col-md-4">
                <label for="inputZip" class="form-label">Percentage</label>
                <input type="text" name="pqPercentage" class="form-control" id="inputZip" />
            </div>

            <div class="col-md-4">
                <label for="inputZip" class="form-label">Attempts</label>
                <input type="text" name="pqAttempts" class="form-control" id="inputZip" />
            </div>

            <div class="col-md-6">
                <label for="inputZip" class="form-label">Upload Marksheet</label><br />
                <div class="input-group">
                    <input type="file" class="form-control" name="pqMarksheet" id="inputGroupFile02" />
                    <!-- <button type="view" class="btn btn-secondary">View Document</button> -->
                </div>
            </div>

            <div class="col-md-3">
                <label for="inputState" class="form-label">Was any gap in this/ Qualification/Course?</label>
                <br />
                <div class="form-check form-check-inline">
                    <input class="form-check-input" type="radio" name="pqHaveGAP" id="exampleRadios1"
                        value="Yes" />
                    <label class="form-check-label" for="exampleRadios1"> Yes </label>
                </div>
                <div class="form-check form-check-inline">
                    <input class="form-check-input" type="radio" name="pqHaveGAP" id="exampleRadios2"
                        value="No" />
                    <label class="form-check-label" for="exampleRadios2"> No </label>
                </div>
            </div>

            <div class="col-md-1"></div>

            <div class="col-md-4">
                <label for="inputZip" class="form-label">If yes, how many years?</label>
                <input type="text" name="pqGAPyears" class="form-control" id="inputZip" />
            </div>

            <br />
            <br />
            

            <div class="d-grid" style="width: auto; align-items: center">
                <button type="submit" class="btn btn-primary">Save and Next</button>
            </div>


            <br />
            <br />
            <br />
            <br />
            <br />
            <legend>Your Saved Past Qualification Details</legend>
        </form>
        <br />

        <table class="table">
            <thead>
                <tr>
                    <th scope="col">Sr. no.</th>
                    <th scope="col">Qualification Level</th>
                    <th scope="col">Stream</th>
                    <th scope="col">Course</th>
                    <th scope="col">Institute State</th>
                    <th scope="col">Board/Univerity</th>
                    <th scope="col">Result</th>
                    <th scope="col">Admission Year</th>
                    <th scope="col">Passing Year</th>
                    <th scope="col">Percentage</th>
                    <th scope="col">View Document</th>
                    <th scope="col">Delete</th>
                </tr>
            </thead>
            <tbody class="table-group-divider">
                <tr>
                    <th scope="row">1</th>
                    <td>CBSE (10 Std)</td>
                    <td>CBSE</td>
                    <td>CBSE</td>
                    <td>Maharashtra</td>
                    <td>CBSE</td>
                    <td>Passed</td>
                    <td>2009</td>
                    <td>2019</td>
                    <td>95.6</td>

                    <td>
                        <button href="#" class="btn btn-secondary">View Document</button>
                    </td>
                    <td><button href="#" class="btn btn-secondary">Delete</button></td>
                </tr>
            </tbody>
        </table>
    </div>
</body>

</html>