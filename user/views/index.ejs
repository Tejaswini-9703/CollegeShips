<!DOCTYPE html>
<html lang="zxx">
	<head>
		<meta charset="UTF-8" />
		<meta
			http-equiv="X-UA-Compatible"
			content="IE=edge"
		/>
		<meta
			name="viewport"
			content="width=device-width, initial-scale=1.0"
		/>
		<!-- title -->
		<title>CollegeShip</title>
		<!-- favicon -->
		<link
			rel="icon"
			type="image/png"
			sizes="32x32"
			href="./asset/page/favicon.png"
		/>
		<link
			rel="icon"
			type="image/png"
			sizes="16x16"
			href="./asset/page/favicon.png"
		/>
		<link
			rel="shortcut icon"
			href="./asset/page/favicon.png"
		/>
		<!-- metatag -->
		<meta name="author" content="Team Squadron" />

		<!-- main style -->
		<link href="css/main.css" rel="stylesheet" />
		<!-- responsive style -->
		<link
			href="css/responsive.css"
			rel="stylesheet"
		/>
		<!-- ion icon-->
		<link
			href="https://unpkg.com/ionicons@4.5.10-0/dist/css/ionicons.min.css"
			rel="stylesheet"
		/>

		<link
			rel="stylesheet"
			href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css"
		/>
		<link
			rel="stylesheet"
			href="./css/faqs.css"
		/>
		<link
			rel="stylesheet"
			href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css"
		/>
	</head>

	<body>
		<!-- header -->
		<header id="headermain">
			<div class="container">
				<div class="row headerwrap">
					<div
						class="col-xs-12 col-md-12 col-lg-2"
					>
						<!-- wrap logo -->
						<div class="headerwrap__logo">
							<img
								src="./asset/page/CollegeShip_logo.png"
								alt="logo"
								class="img-fluid"
							/>
						</div>
						<!-- end wrap logo -->
						<!-- navigation icon  -->
						<div
							class="navicon d-block d-lg-none"
						>
							<div class="navicon__bar"></div>
						</div>
						<!-- end navigation icon  -->
					</div>
					<div
						class="col-lg-10 text-xl-left d-none d-lg-block justify-content-lg-left justify-content-xl-left"
					>
						<!-- navigation desktop -->
						<nav class="navpage text-left">
							<ul class="navpage__wrap">
								<li class="navpage__list">
									<a href="#sectionhero"
										>Intro.</a
									>
								</li>
								<li class="navpage__list">
									<a href="#aboutsection"
										>About Us.</a
									>
								</li>
								<li class="navpage__list">
									<a href="#servicesection"
										>Features.</a
									>
								</li>
								<li class="navpage__list">
									<a href="#portfoliosection"
										>FAQS.</a
									>
								</li>
								<li class="navpage__list">
									<a href="#contactsection"
										>Contact Us.</a
									>
								</li>
								<% if(user) { %>
								<li>
									<a
										href="/logout"
										class="btn linkbtn2 aboutlink"
										style="color: white"
									>
										Logout
									</a>
								</li>
								<% } else { %>
								<li>
									<a
										href="/login"
										class="btn linkbtn2 aboutlink"
										style="color: white"
									>
										Login
									</a>
								</li>
								<li>
									<a
										href="/signup"
										class="btn linkbtn2 aboutlink"
										style="color: white"
									>
										Signup
									</a>
								</li>
								<% } %>
							</ul>
						</nav>
						<!-- end navigation desktop -->
					</div>
				</div>
			</div>
		</header>
		<!-- end header -->
		<!-- mobile nav -->
		<div class="overlay">
			<div class="overlay__navigation">
				<ul class="overlay__listnav">
					<li class="navpage__list">
						<a href="#herosection">Intro.</a>
					</li>
					<li>
						<a href="#aboutsection">About Us.</a>
					</li>
					<li>
						<a href="#servicesection"
							>Services.</a
						>
					</li>
					<li>
						<a href="#portfoliosection"
							>Portfolio.</a
						>
					</li>
					<li>
						<a href="#contactsection">Contact.</a>
					</li>
				</ul>
			</div>
		</div>
		<!-- end mobile nav -->
		<!-- PRELOADER -->

		<!-- main wrap -->
		<main id="mainwrap">
			<!-- section hero -->
			<div id="sectionhero" class="sectionblock">
				<div class="container">
					<div class="row">
						<div
							class="col-lg-6 pr-0 mr-0 pl-lg-0 pr-lg-5 mr-lg-0 mt-3 mt-lg-0 align-items-center justify-content-center align-items-lg-left justify-content-lg-left text-center text-lg-left d-flex order-1 order-lg-0"
						>
							<div class="infohero">
								<p class="infohero__p">
									Hello, <%= user ? user.email :
									""%> Welcome to
								</p>
								<h1 class="infohero__title">
									<span>CollegeShip</span>
								</h1>
								<span id="typed-text"></span>
								<br />
								<!-- <br> -->
								<!-- <br> -->
								<a
									href="/ocr"
									class="btn linkbtn aboutlink mt-4"
								>
									Apply For Scholarships >
								</a>
								<!-- <span class="cursor blink">&nbsp;</span> -->
							</div>
						</div>
						<div
							class="col-lg-6 text-center order-0 order-lg-1 mb-5 mb-lg-0"
						>
							<div class="heroimg">
								<img
									src="./asset/hero/1.png"
									alt="poto"
									class="img-fluid"
								/>
							</div>
						</div>
					</div>
				</div>
				<svg
					xmlns="http://www.w3.org/2000/svg"
					viewBox="0 0 1440 320"
				>
					<path
						fill="#ffffff"
						fill-opacity="1"
						d="M0,128L60,149.3C120,171,240,213,360,192C480,171,600,85,720,85.3C840,85,960,171,1080,197.3C1200,224,1320,192,1380,176L1440,160L1440,320L1380,320C1320,320,1200,320,1080,320C960,320,840,320,720,320C600,320,480,320,360,320C240,320,120,320,60,320L0,320Z"
					></path>
				</svg>
			</div>
			<!-- end section hero -->
			<!-- about section -->
			<div id="aboutsection" class="sectionblock">
				<div class="container">
					<div class="row">
						<div
							class="col-lg-4 text-center text-lg-left"
						>
							<div class="abouthero">
								<h3>About CollegeShip</h3>
							</div>
						</div>
						<div
							class="col-lg-8 pl-lg-5 text-center text-lg-left"
						>
							<div class="infoabout">
								<p class="infoabout__title">
									CollegeShip is an Online-Offline
									Portal for Application of
									Scholarships. For those who have
									internet connectivity, they will
									be using the online website to
									apply for Scholarships. And, for
									students in rural areas who
									don't have access to internet
									will be going to the local Gram
									Panchayat/Village School, and
									will apply from the web
									application installed in it. The
									data will be stored on the
									system in an encrypted format.
									If there is availability of
									internet anytime, then the data
									will be updated automatically to
									the main server, and if not the
									data can be collected in a
									memory device and transferred to
									the server through the online
									website.
								</p>
								<a
									href="#"
									class="btn linkbtn1 aboutlink mt-4"
								>
									Apply For Scholarships >
								</a>
							</div>
						</div>
					</div>
				</div>
			</div>
			<!-- end about -->
			<!-- service section -->
			<div
				id="servicesection"
				class="sectionblock"
			>
				<svg
					xmlns="http://www.w3.org/2000/svg"
					viewBox="0 0 1440 320"
				>
					<path
						fill="#ffffff"
						fill-opacity="1"
						d="M0,96L60,117.3C120,139,240,181,360,186.7C480,192,600,160,720,138.7C840,117,960,107,1080,117.3C1200,128,1320,160,1380,176L1440,192L1440,0L1380,0C1320,0,1200,0,1080,0C960,0,840,0,720,0C600,0,480,0,360,0C240,0,120,0,60,0L0,0Z"
					></path>
				</svg>
				<div class="container">
					<div class="row">
						<div class="col-lg-12">
							<div
								id="sliderservice"
								class="sliderservice__contentinner keen-slider"
							>
								<div
									class="wrapservice keen-slider__slide text-center text-lg-left"
								>
									<i
										class="fa-solid fa-cubes"
									></i>
									<h3
										class="mt-3 wrapservice__title"
									>
										OCR Technology.
									</h3>
									<p>
										Optical Character Recognition
										makes the form filling process
										faster and the candidate
										verification easier.
									</p>
								</div>
								<div
									class="wrapservice keen-slider__slide text-center text-lg-left"
								>
									<i
										class="fa-solid fa-fingerprint"
									></i>
									<h3
										class="mt-3 wrapservice__title"
									>
										Biometric Verification.
									</h3>
									<p>
										For the offline verification,
										we have integrated Biometric
										Verification instead of OTP
										Verification
									</p>
								</div>
								<div
									class="wrapservice keen-slider__slide text-center text-lg-left"
								>
									<i class="fa-solid fa-sms"></i>
									<h3
										class="mt-3 wrapservice__title"
									>
										SMS Updates.
									</h3>
									<p>
										Candidate will get updates
										about his application at every
										stage of process.
									</p>
								</div>
								<div
									class="wrapservice keen-slider__slide text-center text-lg-left"
								>
									<i class="fa-solid fa-plus"></i>
									<h3
										class="mt-3 wrapservice__title"
									>
										Necessary Features.
									</h3>
									<p>
										With all other features, we
										have taken care the UI is easy
										enough and other necessary
										features are present.
									</p>
								</div>
							</div>
						</div>
					</div>
				</div>
				<svg
					xmlns="http://www.w3.org/2000/svg"
					viewBox="0 0 1440 320"
				>
					<path
						fill="#ffffff"
						fill-opacity="1"
						d="M0,96L60,117.3C120,139,240,181,360,197.3C480,213,600,203,720,186.7C840,171,960,149,1080,160C1200,171,1320,213,1380,234.7L1440,256L1440,320L1380,320C1320,320,1200,320,1080,320C960,320,840,320,720,320C600,320,480,320,360,320C240,320,120,320,60,320L0,320Z"
					></path>
				</svg>
			</div>

			<!-- Start Faqs section -->
			<div
				id="portfoliosection"
				class="sectionblock"
			>
				<section>
					<div class="containerr">
						<div class="accordion">
							<div class="accordion-item" id="q1">
								<a
									class="accordion-link"
									href="#q1"
								>
									Who can apply into the DBT
									portal ?
									<i class="icon ion-md-add"></i>
									<i
										class="icon ion-md-remove"
									></i>
								</a>
								<div class="ans">
									<p>
										Any Citizen of India can
										register on the portal.
										Eligibility and disbusement
										will be according to the
										schemes as displayed
									</p>
								</div>
							</div>

							<!-- q2-->
							<div class="accordion-item" id="q2">
								<a
									class="accordion-link"
									href="#q2"
								>
									How to check the Eligibility for
									the scheme ?
									<i class="icon ion-md-add"></i>
									<i
										class="icon ion-md-remove"
									></i>
								</a>
								<div class="ans">
									<p>
										The basic objective of the
										Check Eligibility is to fetch
										relevant that are applicable
										to the Applicant on the basis
										of the input provided by the
										applicant at the time of
										profile filling.
									</p>
								</div>
							</div>

							<!--q3-->
							<div class="accordion-item" id="q3">
								<a
									class="accordion-link"
									href="#q3"
								>
									How would i know thad DBT funds
									have come to my account?
									<i class="icon ion-md-add"></i>
									<i
										class="icon ion-md-remove"
									></i>
								</a>
								<div class="ans">
									<p>
										We recommend that you update
										your mobail number in bank.The
										bank will send out SMS alerts
										when you get DBT in account or
										you transact on your account.
									</p>
								</div>
							</div>

							<!-- Q4-->

							<div class="accordion-item" id="q4">
								<a
									class="accordion-link"
									href="#q4"
								>
									What is the Maz File Size for
									uploading document?
									<i class="icon ion-md-add"></i>
									<i
										class="icon ion-md-remove"
									></i>
								</a>
								<div class="ans">
									<p>
										The maximum file size a user
										can upload is 250 KB.
									</p>
								</div>
							</div>

							<!--Q5-->
							<div class="accordion-item" id="q5">
								<a
									class="accordion-link"
									href="#q5"
								>
									How to check the status of the
									application after submission ?
									<i class="icon ion-md-add"></i>
									<i
										class="icon ion-md-remove"
									></i>
								</a>
								<div class="ans">
									<p>
										Login to the DBT portal,under
										the Applicatio tracking
										menu,the applicant can see the
										status of their application by
										entering the valid Application
										ID.
									</p>
								</div>
							</div>

							<!--Q6-->
							<div class="accordion-item" id="q6">
								<a
									class="accordion-link"
									href="#q6"
								>
									What are the different file
									formats which can be accepted ?
									<i class="icon ion-md-add"></i>
									<i
										class="icon ion-md-remove"
									></i>
								</a>
								<div class="ans">
									<p>JPG and PNG format</p>
								</div>
							</div>

							<!-- Q7-->
						</div>
					</div>
				</section>
			</div>

			<!-- end FAQS section -->
			<div
				id="contactsection"
				class="sectionblock"
			>
				<svg
					xmlns="http://www.w3.org/2000/svg"
					viewBox="0 0 1440 320"
				>
					<path
						fill="#ffffff"
						fill-opacity="1"
						d="M0,96L60,117.3C120,139,240,181,360,186.7C480,192,600,160,720,138.7C840,117,960,107,1080,117.3C1200,128,1320,160,1380,176L1440,192L1440,0L1380,0C1320,0,1200,0,1080,0C960,0,840,0,720,0C600,0,480,0,360,0C240,0,120,0,60,0L0,0Z"
					></path>
				</svg>
				<div class="container">
					<div class="row">
						<div class="col-lg-6 pr-lg-5">
							<div class="contactdetail">
								<h3
									class="contactdetail__title mb-4"
								>
									Contact Us
								</h3>
								<p>
									Feel free to Contact us on the
									mobile or email given below.
								</p>
								<ul class="contactdetail__list">
									<li>
										<span
											class="contactdetail__span"
										>
											Vishwakarma Institute of
											Technology, Pune
										</span>
									</li>
									<li>
										<span
											class="contactdetail__span"
										>
											+91 7558612763
										</span>
									</li>
									<li>
										<span
											class="contactdetail__span"
										>
											collegeship@gmail.com
										</span>
									</li>
								</ul>
							</div>
						</div>
						<div class="col-lg-6">
							<div class="contactform">
								<!-- form contact -->
								<form
									action="#"
									id="formcontact"
									class="contactform"
								>
									<!-- input contact wrap -->
									<div
										class="contactform__form-group row"
									>
										<div class="col-lg-12 pt-4">
											<label
												for="yourname"
												class="pb-2"
												>Input your name</label
											>
											<input
												type="text"
												id="yourname"
												class="contactform__inputtext input-control"
												placeholder="Your name"
												name="name"
												data-name="name"
											/>
										</div>
										<!-- input contact wrap -->
										<div class="col-lg-12 pt-4">
											<label
												for="yourmail"
												class="pb-2"
												>Input your mail</label
											>
											<input
												type="email"
												id="yourmail"
												class="contactform__inputtext input-control"
												placeholder="Your email address"
												name="email"
												data-name="email"
											/>
										</div>
										<!-- input contact wrap -->
										<div class="col-lg-12 pt-4">
											<label
												for="comment"
												class="pb-2"
												>Input your message</label
											>
											<textarea
												class="contactform__comentarea input-control"
												id="comment"
												placeholder="Your comment"
												name="comment"
												data-name="comment"
											></textarea>
											<button
												id="submitbutton"
												class="btn mt-5 contactform__submitbuton linkbtn mt-5 mb-3"
												type="submit"
												style="width: 540px"
											>
												Submit now
											</button>
										</div>
									</div>
								</form>
							</div>
						</div>
					</div>
				</div>
			</div>
			<!-- end contact section -->
		</main>
		<!-- end main wrap -->
		<!-- footer section -->
		<footer id="footerwrap" class="pt-4">
			<div class="container">
				<div class="row">
					<div
						class="col-lg-12 text-center footwrap"
					>
						<p>
							<b>CollegeShip</b>
							<span class="years"> </span> BY TEAM
							SQUADRON
						</p>
					</div>
				</div>
			</div>
		</footer>
		<!-- end footer section-->
		<!-- keen slider -->
		<script src="./javascript/vendor/keen-slider.js"></script>
		<!-- glightbox -->
		<script src="./javascript/vendor/glightbox.js"></script>
		<!-- shufle -->
		<script src="./javascript/vendor/shufle.js"></script>
		<!-- script -->
		<script src="./javascript/main.js"></script>
		<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js"></script>
	</body>
</html>
